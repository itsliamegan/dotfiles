[alias]
  a = commit --amend
  anoe = commit --amend --no-edit
  co = checkout
  cm = commit -m
  dc = diff --cached HEAD
[core]
  attributesfile = ~/.gitattributes
  pager = diff-so-fancy | less --tabs=2 -FX
  excludesfile = ~/.gitignore
[init]
  defaultBranch = main
